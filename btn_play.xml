<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center">
        <shape android:shape="rectangle">
            <size
                android:height="30dp"
                android:width="30dp"/>
            <corners android:radius="40dp"/>
            <solid android:color="#f0a"/>
            <stroke android:width="0.5dp" android:color="#d36"/>
        </shape>
    </item>
    <!-- Start block -->
    <item
        android:gravity="center_horizontal"
        android:start="-6dp"
        android:top="5dp"
        android:bottom="5dp">
        <shape android:shape="rectangle">
            <size
                android:height="14dp"
                android:width="5dp"/>
            <corners android:radius="10dp"/>
            <solid android:color="#fff"/>
        </shape>
    </item>
    <!-- Top block -->
    <item
        android:gravity="center_vertical"
        android:top="-8dp"
        android:start="9dp"
        android:end="5dp">
        <rotate
            android:fromDegrees="38">
            <shape android:shape="rectangle">
                <size
                    android:height="5dp"
                    android:width="10dp"/>
                <corners android:radius="10dp"/>
                <solid android:color="#fff"/>
            </shape>
        </rotate>
    </item>
    <!-- Bottom block -->
    <item
        android:gravity="center_vertical"
        android:bottom="-8dp"
        android:start="9dp"
        android:end="5dp">
        <rotate
            android:fromDegrees="-38">
            <shape android:shape="rectangle">
                <size
                    android:height="5dp"
                    android:width="10dp"/>
                <corners android:radius="10dp"/>
                <solid android:color="#fff"/>
            </shape>
        </rotate>
    </item>
    <!-- Filler -->
    <item
        android:gravity="center_vertical|center_horizontal">
        <shape android:shape="rectangle">
            <size
                android:height="10dp"
                android:width="10dp"/>
            <solid android:color="#fff"/>
        </shape>
    </item>
</layer-list>